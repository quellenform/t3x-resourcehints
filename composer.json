{
	"name": "quellenform/t3x-resourcehints",
	"type": "typo3-cms-extension",
	"description": "Adds external hosts to the HTML-header for DNS-refetching.",
	"keywords": [
		"TYPO3",
		"extension"
	],
	"license": [
		"GPL-3.0-or-later"
	],
	"homepage": "https://extensions.typo3.org/extension/resourcehints",
	"support": {
		"source": "https://github.com/quellenform/t3x-resourcehints",
		"issues": "https://github.com/quellenform/t3x-resourcehints/issues"
	},
	"authors": [
		{
			"name": "Stephan Kellermayr",
			"email": "typo3@quellenform.at",
			"role": "Developer",
			"homepage": "https://www.quellenform.at/"
		}
	],
	"require": {
		"php": ">=7.2.0",
		"typo3/cms-core": ">=10.4 <15 || 15.*.*@dev"
	},
	"autoload": {
		"psr-4": {
			"Quellenform\\Resourcehints\\": "Classes"
		}
	},
	"replace": {
		"typo3-ter/resourcehints": "self.version"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://composer.typo3.org/"
		}
	],
	"extra": {
		"typo3/cms": {
			"extension-key": "resourcehints"
		}
	}
}
